    "grunt-cli": "0.1.13",
    "grunt-eslint": "17.2.0",
    "grunt-mocha-test": "0.12.7",
    "istanbul": "0.3.21",
    "load-grunt-tasks": "3.3.0",
    "mocha-lcov-reporter": "0.0.2",
    "should": "7.1.0"
  },
  "engines": {
    "node": ">= 0.10.0"
