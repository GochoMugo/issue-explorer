{
  "name": "issue-explorer",
  "version": "0.3.1",
  "description": "Explore Github issues from your terminal",
  "main": "lib/index.js",
  "bin": {
    "issue-explorer": "bin/issue-explorer",
    "gie": "bin/issue-explorer"
  },
  "scripts": {
    "build": "babel src -D --out-dir .",
    "prepublish": "npm run build",
