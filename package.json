  "bugs": {
    "url": "https://github.com/GochoMugo/issue-explorer/issues"
  },
  "homepage": "https://github.com/GochoMugo/issue-explorer#readme",
  "dependencies": {
    "blessed": "0.1.81",
    "blessed-contrib": "2.3.3",
    "cli-output": "0.1.0",
    "fixed-object": "0.0.0",
    "github": "0.2.4",
    "github-slug": "0.0.2",
    "inquirer": "0.9.0",
    "lodash": "3.10.1",
    "moment": "2.10.6",
    "open": "0.0.5",
